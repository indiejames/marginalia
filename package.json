{
    "name": "marginalia",
    "displayName": "marginalia",
    "description": "Add margin notest fo your code.",
    "version": "0.0.1",
    "publisher": "jamesnorton",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.annotate"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.annotate",
            "title": "Add Note"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "uuid": "^3.1.0"
    }
}